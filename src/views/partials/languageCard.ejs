<div class="col-xl-3 col-lg-6 col-md-6 col-sm-6">
    <div class="card bank-card pull-up">
        <div class="card-content">
            <div class="card-body">
                <div class="row">
                    <div class="col-5 text-left">
                        <img src="<%- language.icon  %> "/>
                        <h5 class="mt-1 text-bold-500 <%- language.color  %> "><b><%- language.name %></b></h5>
                    </div>
                    <div class="col-7 my-auto">
                        <div class="progress">
                            <div  class="progress-bar bg-<%- language.color  %>"  role="progressbar" aria-valuenow="<%-language.value%>" aria-valuemin="0" aria-valuemax="100" style="width:<%- language.value%>%" ><%- language.value  %>   % </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>